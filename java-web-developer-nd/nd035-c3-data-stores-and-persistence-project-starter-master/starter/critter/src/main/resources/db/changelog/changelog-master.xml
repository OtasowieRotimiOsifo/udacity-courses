<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xmlns:pro="http://www.liquibase.org/xml/ns/pro"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd
http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-4.1.xsd">
    <changeSet id="20220210" author="oosifo">
        <createTable tableName="owner">
            <column name="id" type="BIGINT(8)" autoIncrement="true">
                <constraints nullable="false" primaryKey="true" primaryKeyName="owner_id_pk" />
            </column>
            <column name="name" type="varchar(250)">
                <constraints nullable="false"/>
            </column>
            <column name="phoneNumber" type="varchar(50)">
                <constraints nullable="false"/>
            </column>
            <column name="notes" type="varchar(250)">
                <constraints nullable="false"/>
            </column>
        </createTable>
        <addAutoIncrement tableName="owner" columnName="id" columnDataType="bigint" startWith="1" incrementBy="1" />
    </changeSet>
</databaseChangeLog>